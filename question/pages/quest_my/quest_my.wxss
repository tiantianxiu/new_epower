page{
  background-color: #ffffff!important;
}
.my-quest{
  padding: 50rpx 40rpx;
}
.list{
  border-bottom: #e5e5e5 solid 2rpx;
  display: flex;
  height: 100rpx;
  line-height: 100rpx;
}
.list text{
  width: 50%;
  color: #676767;
  font-size: 30rpx;
}
text.navigator-arrow{
  text-align: right;
  font-size: 38rpx;
  color: #fd3600;
  padding-right:60rpx;
}
text.navigator-arrow::after{
  width: 20rpx;
  height: 20rpx;
   border-width: 4rpx 4rpx 0 0;
   margin-top: -12rpx;
}