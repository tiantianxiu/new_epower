image {
  width: 100%;
  height: 100%;
}

.appear-list {
  margin-bottom: 6rpx;
  padding: 30rpx 14rpx;
  background: #fff;
  display: flex;
}

.appear-list .title {
  font-weight: 700;
  font-size: 26rpx;
  color: #777;
  line-height: 52rpx;
  height: 52rpx;
}

.appear-list .content {
  color: #00c481;
  font-size: 22rpx;
  width: 85%;
  text-align: center;
  line-height: 52rpx;
}

.half {
  display: flex;
  width: 50%;
}

.half50 {
  width: 50%;
}

.half60 {
  width: 60%;
}

.half40 {
  width: 30%;
  display: inline-block;
}

.half .navigator-arrow {
  width: 56%;
  text-align: right;
  padding-right: 36rpx;
  line-height: 48rpx;
  font-size: 22rpx;
  overflow: hidden; /*超出部分隐藏*/
  white-space: nowrap; /*不换行*/
  text-overflow: ellipsis; /*超出部分文字以...显示*/
  margin-right: 20rpx;
}

.half .navigator-arrow:after {
  top: 48%;
  text-align: right;
}

.half .title {
  width: 54%;
}

.half:nth-child(even) .title {
  border-left: 2rpx solid #d2d2d2;
  padding-left: 6%;
}

.input {
  padding: 0 15% 0 6%;
  width: 56%;
  position: relative;
}

.half text {
  font-weight: 600;
  color: #7e7e7e;
  position: absolute;
  top: 4rpx;
  left: 80%;
  font-size: 26rpx;
}

.input-special {
  width: 70%;
  font-size: 24rpx;
  display: flex;
  justify-content: center;
  align-items: Center;
  padding: 0;
}

.input-special text {
  font-weight: 600;
  color: #7e7e7e;
}

.input-special input {
  width: 50%;
  display: inline-block;
  padding: 0 10rpx;
  background: #eee;
  margin: 0 20rpx 0 40rpx;
  text-align: center;
}

input {
  text-align: right;
}

.placeholder {
  text-align: right;
  color: #a3a3a3;
  font-size: 24rpx;
}

.placeholder-special {
  text-align: center;
}

.appear-list.score-list {
  margin-bottom: 2rpx;
  position: relative;
  display: flex;
}

.score-list .grade {
  font-weight: 600;
  color: #00c481;
  line-height: 76rpx;
  font-size: 26rpx;
}

.appear-list.score-list .grade {
  color: #f50000;
}

.score-list {
  background: #fff;
  padding: 16rpx 30rpx 20rpx 20rpx;
  display: none;
}

.score-list.score-list-image {
  display: flex;
}

.score-list.active {
  display: flex;
}

.score-list.score-list-detail {
  padding: 40rpx 30rpx;
}

.appear-list.score-list .title {
  font-size: 24rpx;
}

.score-list .title {
  color: #767676;
  width: 16%;
  line-height: 76rpx;
  font-size: 24rpx;
}

.progress {
  width: 68%;
  position: relative;
}

.progress-absolute {
  position: absolute;
  width: 20%;
  height: 100%;
  right: 0;
  top: 0;
}

slider {
  position: relative;
  line-height: 80rpx;
}

.mark-list {
  position: absolute;
  bottom: -24rpx;
  left: 0;
  width: 100%;
  height: 30rpx;
  display: flex;
}

.mark {
  border-right: 4rpx solid #fff;
  height: 200%;
  position: absolute;
  width: 2rpx;
  bottom: 0;
  line-height: 86rpx;
  color: #9c9c9c;
  font-size: 18rpx;
}

.markz {
  left: 0;
  border-right: 0;
}

.mark1 {
  left: 20%;
}

.mark2 {
  left: 40%;
}

.mark3 {
  left: 60%;
}

.mark4 {
  left: 80%;
}

.mark5 {
  right: 8rpx;
  border-right: 0;
}

.score-list .header {
  background: #89c997;
  color: #fff;
  width: 6%;
  padding: 0 2%;
  font-size: 30rpx;
  display: flex;
  text-align: center;
}

.score-list .header text {
  margin: auto;
}

.evaluate-ul {
  width: 66%;
  padding: 0 50rpx;
  display: inline-block;
}

.evaluate {
  display: flex;
  font-size: 30rpx;
  padding-bottom: 18rpx;
  margin: 32rpx;
  border-bottom: 2rpx solid #dad9d9;
  position: relative;
}

.evaluate:last-child {
  border: none;
}

.evaluate text {
  font-weight: 600;
  color: #a3a3a3;
  padding-right: 30rpx;
}

.evaluate .li {
  border-radius: 12rpx;
  border-width: 2rpx;
  border-style: solid;
  padding: 2rpx 10rpx;
  min-width: 56rpx;
  text-align: center;
  position: absolute;
  bottom: 18rpx;
  line-height: 155%;
  font-size: 26rpx;
}

.evaluate .li {
  border-color: #aaa;
  color: #aaa;
  left: 72%;
}

.evaluate .li.unbad {
  /* border-color: #f8b551;
  color: #f8b551; */
  left: 40%;
}

.enlarge-tap {
  position: absolute;
  top: -73%;
  left: -34%;
  width: 170%;
  height: 222%;
  opacity: 0;
}

.evaluate .unbad.active {
  background: #f8b551;
  color: #fff;
  font-weight: 500;
  border-color: #f8b551;
}

.evaluate .bad.active {
  background: #aaa;
  color: #fff;
  font-weight: 500;
}

.li icon {
  position: absolute;
  bottom: -28rpx;
  right: -8rpx;
}

.btn {
  background: #00c481;
  color: #fff;
  font-size: 30rpx;
  text-align: center;
  width: 100%;
  height: 100rpx;
  line-height: 100rpx;
  border-bottom: 2rpx solid #fff;
}

/* .cars-url{
  
} */

.url-li {
  width: 29%;
  margin-left: 3%;
  margin-bottom: 40rpx;
  position: relative;
  text-align: center;
  display: inline-block;
  height: 240rpx;
}

.url-li icon {
  position: absolute;
  right: -24rpx;
  top: -22rpx;
}

.pr-icon {
  width: 20rpx;
  height: 20rpx;
  position: absolute;
  right: 18rpx;
  top: 28rpx;
}

.pr-icon image {
  transition: all 0.2s;
}

.pr-icon.active image {
  transform: rotate(-90deg);
}

.textarea textarea {
  font-size: 24rpx;
  background: #eee;
  width: 90%;
  padding: 3%;
  height: 200rpx;
}
.border-red{
  border: 2rpx solid  #f00;
}