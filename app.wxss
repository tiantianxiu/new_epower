/* widget */

.btn-area {
  margin-top: 100rpx;
  box-sizing: border-box;
  width: 100%;
}

.navigator-box {
  opacity: 0;
  position: relative;
  background-color: #fff;
  line-height: 1.41176471;
  font-size: 34rpx;
  transform: translateY(-50%);
  transition: 0.3s;
}

.navigator-box-show {
  opacity: 1;
  transform: translateY(0);
}

.navigator {
  padding: 20rpx 30rpx;
  position: relative;
  display: flex;
  align-items: center;
  border-bottom: 3rpx solid #eee;
}

.navigator:first-child:before {
  display: none;
}

.navigator-text {
  flex: 1;
  line-height: 60rpx;
}

.navigator-img {
  flex: 1;
  line-height: 0;
}

.navigator-img image {
  width: 60rpx;
  height: 60rpx;
}

.navigator-arrow {
  padding-right: 40rpx;
  position: relative;
  font-size: 24rpx;
  color: #a4a4a4;
}

.navigator-arrow::after {
  content: " ";
  display: inline-block;
  height: 12rpx;
  width: 12rpx;
  border-width: 1rpx 1rpx 0 0;
  border-color: #888;
  border-style: solid;
  transform: matrix(0.71, 0.71, -0.71, 0.71, 0, 0);
  position: absolute;
  top: 50%;
  margin-top: -6rpx;
  right: 10rpx;
}

.navigator-hover {
  color: #00c481;
  background: none;
}

.tag-wrap {
  margin-top: 14rpx;
  padding-top: 25rpx;
  padding-bottom: 25rpx;
}

.tag-item {
  display: flex;
  height: 50rpx;
  line-height: 50rpx;
  padding: 0 25rpx;
  font-size: 22rpx;
  vertical-align: middle;
}

.tag-item.active {
