/*swiper-滚动图*/

.swiper-wrap {
  margin-top: 0;
  background-color: #fff;
  padding: 0rpx 30rpx 20rpx;
  position: relative;
}

.slide-image {
  width: 100%;
  height: 342rpx;
  background: center center no-repeat;
  background-size: cover;
  position: relative;
}

.slide-text {
  font-size: 24rpx;
  color: #fff;
  background-color: rgba(0, 0, 0, 0.4);
  position: absolute;
  bottom: 0;
  left: 0;
  right: 0;
  line-height: 1.4;
  padding: 10rpx 30rpx;
}

.swiper-wrap .dots {
  position: absolute;
  left: 0;
  right: 0;
  bottom: 40rpx;
  display: flex;
  justify-content: center;
}

.swiper-wrap .dots .dot {
  margin: 0 8rpx;
  width: 14rpx;
  height: 14rpx;
  background: rgba(255, 255, 255, 0.5);
  border-radius: 8rpx;
  transition: all 0.6s;
}

.swiper-wrap .dots .dot.active {
  background: #00c481;
}

